s:sequence><xs:any namespace="http://schemas.microsoft.com/sqlserver/DMF/2007/08" processContents="skip" minOccurs="0" maxOccurs="unbounded" /></xs:sequence></xs:complexType></xs:element><DMF:bufferData xmlns:DMF="http://schemas.microsoft.com/sqlserver/DMF/2007/08"><instances xmlns:sfc="http://schemas.microsoft.com/sqlserver/sfc/serialization/2007/08"><document><docinfo><aliases><alias>/PolicyStore/Policy/EPM - Replication Latency Greater Than 15 minutes</alias></aliases><sfc:version DomainVersion="3" /></docinfo><data><DMF:Policy xmlns:DMF="http://schemas.microsoft.com/sqlserver/DMF/2007/08" xmlns:sfc="http://schemas.microsoft.com/sqlserver/sfc/serialization/2007/08" xmlns:sml="http://schemas.serviceml.org/sml/2007/02" xmlns:xs="http://www.w3.org/2001/XMLSchema"><DMF:Parent><sfc:Reference sml:ref="true"><sml:Uri>/PolicyStore</sml:Uri></sfc:Reference></DMF:Parent><DMF:PolicyCondition><sfc:Reference sml:ref="true"><sml:Uri>/PolicyStore/Condition/EPM - Replication Latency &gt; 15 mins</sml:Uri></sfc:Reference></DMF:PolicyCondition><DMF:PolicyObjectSet><sfc:Reference sml:ref="true"><sml:Uri>/PolicyStore/ObjectSet/EPM - Replication Latency Greater Than 15 minutes__ObjectSet</sml:Uri></sfc:Reference></DMF:PolicyObjectSet><DMF:PolicyRootCondition><sfc:Reference sml:ref="true"><sml:Uri>/PolicyStore/Condition/EPM - Is Replicated</sml:Uri></sfc:Reference></DMF:PolicyRootCondition><DMF:PolicyPolicyCategory><sfc:Reference sml:ref="true"><sml:Uri>/PolicyStore/PolicyCategory/EPM_b Performance</sml:Uri></sfc:Reference></DMF:PolicyPolicyCategory><DMF:Name type="string">EPM - Replication Latency Greater Than 15 minutes</DMF:Name><DMF:Description type="string" /><DMF:Condition type="string">EPM - Replication Latency &gt; 15 mins</DMF:Condition><DMF:ObjectSet type="string">EPM - Replication Latency Greater Than 15 minutes_ObjectSet</DMF:ObjectSet><DMF:RootCondition type="string">EPM - Is Replicated</DMF:RootCondition><DMF:PolicyCategory type="string">EPM: Performance</DMF:PolicyCategory><DMF:Enabled type="boolean">false</DMF:Enabled><DMF:AutomatedPolicyEvaluationMode type="AutomatedPolicyEvaluationMode">CheckOnSchedule</DMF:AutomatedPolicyEvaluationMode><DMF:ScheduleUid type="guid">3853c052-9621-4141-b856-3677fab4a161</DMF:ScheduleUid><DMF:HelpText type="string" /><DMF:HelpLink type="string" /><DMF:ActiveEndDate type="dateTime">9999-12-31T00:00:00</DMF:ActiveEndDate><DMF:ActiveEndTimeOfDay type="long">863990000000</DMF:ActiveEndTimeOfDay><DMF:ActiveStartDate type="dateTime">2008-07-09T00:00:00</DMF:ActiveStartDate><DMF:ActiveStartTimeOfDay type="long">0</DMF:ActiveStartTimeOfDay><DMF:FrequencyInterval type="int">1</DMF:FrequencyInterval><DMF:FrequencyRecurrenceFactor type="int">0</DMF:FrequencyRecurrenceFactor><DMF:FrequencyRelativeIntervals type="FrequencyRelativeIntervals" /><DMF:FrequencySubDayInterval type="int">10</DMF:FrequencySubDayInterval><DMF:FrequencySubDayTypes type="FrequencySubDayTypes">Minute</DMF:FrequencySubDayTypes><DMF:FrequencyTypes type="FrequencyTypes">Daily</DMF:FrequencyTypes><DMF:Schedule type="string">CollectorSchedule_Every_10min</DMF:Schedule></DMF:Policy></data></document><document><docinfo><aliases><alias>/PolicyStore/ObjectSet/EPM - Replication Latency Greater Than 15 minutes__ObjectSet</alias></aliases><sfc:version DomainVersion="3" /></docinfo><data><DMF:ObjectSet xmlns:DMF="http://schemas.microsoft.com/sqlserver/DMF/2007/08" xmlns:sfc="http://schemas.microsoft.com/sqlserver/sfc/serialization/2007/08" xmlns:sml="http://schemas.serviceml.org/sml/2007/02" xmlns:xs="http://www.w3.org/2001/XMLSchema"><DMF:TargetSets><sfc:Collection><sfc:Reference sml:ref="true"><sml:Uri>/PolicyStore/ObjectSet/EPM - Replication Latency Greater Than 15 minutes__ObjectSet/TargetSet/Server_/Database</sml:Uri></sfc:Reference></sfc:Collection></DMF:TargetSets><DMF:Parent><sfc:Reference sml:ref="true"><sml:Uri>/PolicyStore</sml:Uri></sfc:Reference></DMF:Parent><DMF:Name type="string">EPM - Replication Latency Greater Than 15 minutes_ObjectSet</DMF:Name><DMF:Facet type="string">Database</DMF:Facet></DMF:ObjectSet></data></document><document><docinfo><aliases><alias>/PolicyStore/Condition/EPM - Replication Latency &gt; 15 mins</alias></aliases><sfc:version DomainVersion="3" /></docinfo><data><DMF:Condition xmlns:DMF="http://schemas.microsoft.com/sqlserver/DMF/2007/08" xmlns:sfc="http://schemas.microsoft.com/sqlserver/sfc/serialization/2007/08" xmlns:sml="http://schemas.serviceml.org/sml/2007/02" xmlns:xs="http://www.w3.org/2001/XMLSchema"><DMF:Parent><sfc:Reference sml:ref="true"><sml:Uri>/PolicyStore</sml:Uri></sfc:Reference></DMF:Parent><DMF:Expression type="string">&lt;Operator&gt;&lt;?char 13?&gt;
  &lt;TypeClass&gt;Bool&lt;/TypeClass&gt;&lt;?char 13?&gt;
  &lt;OpType&gt;EQ&lt;/OpType&gt;&lt;?char 13?&gt;
  &lt;Count&gt;2&lt;/Count&gt;&lt;?char 13?&gt;
  &lt;Function&gt;&lt;?char 13?&gt;
    &lt;TypeClass&gt;String&lt;/TypeClass&gt;&lt;?char 13?&gt;
    &lt;FunctionType&gt;ExecuteSql&lt;/FunctionType&gt;&lt;?char 13?&gt;
    &lt;ReturnType&gt;String&lt;/ReturnType&gt;&lt;?char 13?&gt;
    &lt;Count&gt;2&lt;/Count&gt;&lt;?char 13?&gt;
    &lt;Constant&gt;&lt;?char 13?&gt;
      &lt;TypeClass&gt;String&lt;/TypeClass&gt;&lt;?char 13?&gt;
      &lt;ObjType&gt;System.String&lt;/ObjType&gt;&lt;?char 13?&gt;
      &lt;Value&gt;String&lt;/Value&gt;&lt;?char 13?&gt;
    &lt;/Constant&gt;&lt;?char 13?&gt;
    &lt;Constant&gt;&lt;?char 13?&gt;
      &lt;TypeClass&gt;String&lt;/TypeClass&gt;&lt;?char 13?&gt;
      &lt;ObjType&gt;System.String&lt;/ObjType&gt;&lt;?char 13?&gt;
      &lt;Value&gt;&amp;lt;?char 13?&amp;gt;&lt;?char 13?&gt;
SET NOCOUNT ON&amp;lt;?char 13?&amp;gt;&lt;?char 13?&gt;
&amp;lt;?char 13?&amp;gt;&lt;?char 13?&gt;
CREATE TABLE #results&amp;lt;?char 13?&amp;gt;&lt;?char 13?&gt;
(&amp;lt;?char 13?&amp;gt;&lt;?char 13?&gt;
	agent VARCHAR(64),&amp;lt;?char 13?&amp;gt;&lt;?char 13?&gt;
	latency_in_minutes INT&amp;lt;?char 13?&amp;gt;&lt;?char 13?&gt;
)&amp;lt;?char 13?&amp;gt;&lt;?char 13?&gt;
&amp;lt;?char 13?&amp;gt;&lt;?char 13?&gt;
DECLARE @milliseconds INT,&amp;lt;?char 13?&amp;gt;&lt;?char 13?&gt;
				@distribution_agent_id INT,&amp;lt;?char 13?&amp;gt;&lt;?char 13?&gt;
				@latency INT,&amp;lt;?char 13?&amp;gt;&lt;?char 13?&gt;
				@strng VARCHAR(2048),&amp;lt;?char 13?&amp;gt;&lt;?char 13?&gt;
				@minutes INT&amp;lt;?char 13?&amp;gt;&lt;?char 13?&gt;
&amp;lt;?char 13?&amp;gt;&lt;?char 13?&gt;
SET @minutes = 15&amp;lt;?char 13?&amp;gt;&lt;?char 13?&gt;
SET @milliseconds = (@minutes*60)*1000&amp;lt;?char 13?&amp;gt;&lt;?char 13?&gt;
&amp;lt;?char 13?&amp;gt;&lt;?char 13?&gt;
DECLARE distribution_agents CURSOR FOR&amp;lt;?char 13?&amp;gt;&lt;?char 13?&gt;
	SELECT id&amp;lt;?char 13?&amp;gt;&lt;?char 13?&gt;
	FROM distribution.dbo.msdistribution_agents&amp;lt;?char 13?&amp;gt;&lt;?char 13?&gt;
&amp;lt;?char 13?&amp;gt;&lt;?char 13?&gt;
OPEN distribution_agents&amp;lt;?char 13?&amp;gt;&lt;?char 13?&gt;
&amp;lt;?char 13?&amp;gt;&lt;?char 13?&gt;
FETCH NEXT FROM distribution_agents INTO @distribution_agent_id&amp;lt;?char 13?&amp;gt;&lt;?char 13?&gt;
&amp;lt;?char 13?&amp;gt;&lt;?char 13?&gt;
WHILE @@fetch_status = 0&amp;lt;?char 13?&amp;gt;&lt;?char 13?&gt;
	BEGIN&amp;lt;?char 13?&amp;gt;&lt;?char 13?&gt;
		SET @latency = (SELECT delivery_latency &amp;lt;?char 13?&amp;gt;&lt;?char 13?&gt;
										FROM distribution.dbo.msdistribution_history &amp;lt;?char 13?&amp;gt;&lt;?char 13?&gt;
										WHERE time =(	SELECT MAX(time)&amp;lt;?char 13?&amp;gt;&lt;?char 13?&gt;
																	FROM distribution.dbo.msdistribution_history &amp;lt;?char 13?&amp;gt;&lt;?char 13?&gt;
																	WHERE agent_id = @distribution_agent_id)&amp;lt;?char 13?&amp;gt;&lt;?char 13?&gt;
											AND @distribution_agent_id = agent_id)&amp;lt;?char 13?&amp;gt;&lt;?char 13?&gt;
		IF @latency &amp;gt; @milliseconds&amp;lt;?char 13?&amp;gt;&lt;?char 13?&gt;
			BEGIN&amp;lt;?char 13?&amp;gt;&lt;?char 13?&gt;
				INSERT INTO #results&amp;lt;?char 13?&amp;gt;&lt;?char 13?&gt;
					SELECT  name, ((@latency/1000)/60)&amp;lt;?char 13?&amp;gt;&lt;?char 13?&gt;
					FROM distribution.dbo.msdistribution_agents &amp;lt;?char 13?&amp;gt;&lt;?char 13?&gt;
					WHERE @distribution_agent_id = id&amp;lt;?char 13?&amp;gt;&lt;?char 13?&gt;
			END&amp;lt;?char 13?&amp;gt;&lt;?char 13?&gt;
&amp;lt;?char 13?&amp;gt;&lt;?char 13?&gt;
		FETCH NEXT FROM distribution_agents	INTO @distribution_agent_id&amp;lt;?char 13?&amp;gt;&lt;?char 13?&gt;
	END&amp;lt;?char 13?&amp;gt;&lt;?char 13?&gt;
&amp;lt;?char 13?&amp;gt;&lt;?char 13?&gt;
CLOSE distribution_agents&amp;lt;?char 13?&amp;gt;&lt;?char 13?&gt;
DEALLOCATE distribution_agents&amp;lt;?char 13?&amp;gt;&lt;?char 13?&gt;
&amp;lt;?char 13?&amp;gt;&lt;?char 13?&gt;
SELECT @strng = ISNULL(@strng, '''') + agent + '' is experiencing '' + CONVERT(VARCHAR(32),latency_in_minutes) + '' minutes of latency.'' + CHAR(10) &amp;lt;?char 13?&amp;gt;&lt;?char 13?&gt;
FROM #results&amp;lt;?char 13?&amp;gt;&lt;?char 13?&gt;
&amp;lt;?char 13?&amp;gt;&lt;?char 13?&gt;
SELECT ISNULL(LEFT(RTRIM(@strng), (LEN(@strng) - 1)),'''') AS [policy_violation_message]&amp;lt;?char 13?&amp;gt;&lt;?char 13?&gt;
&amp;lt;?char 13?&amp;gt;&lt;?char 13?&gt;
DROP TABLE #results&amp;lt;?char 13?&amp;gt;&lt;?char 13?&gt;
&amp;lt;?char 13?&amp;gt;&lt;?char 13?&gt;
SET NOCOUNT OFF&amp;lt;?char 13?&amp;gt;&lt;?char 13?&gt;
&lt;/Value&gt;&lt;?char 13?&gt;
    &lt;/Constant&gt;&lt;?char 13?&gt;
  &lt;/Function&gt;&lt;?char 13?&gt;
  &lt;Constant&gt;&lt;?char 13?&gt;
    &lt;TypeClass&gt;String&lt;/TypeClass&gt;&lt;?char 13?&gt;
    &lt;ObjType&gt;System.String&lt;/ObjType&gt;&lt;?char 13?&gt;
    &lt;Value /&gt;&lt;?char 13?&gt;
  &lt;/Constant&gt;&lt;?char 13?&gt;
&lt;/Operator&gt;</DMF:Expression><DMF:Name type="string">EPM - Replication Latency &gt; 15 mins</DMF:Name><DMF:Description type="string" /><DMF:Facet type="string">Database</DMF:Facet></DMF:Condition></data></document><document><docinfo><aliases><alias>/PolicyStore/Condition/EPM - Is Replicated</alias></aliases><sfc:version DomainVersion="3" /></docinfo><data><DMF:Condition xmlns:DMF="http://schemas.microsoft.com/sqlserver/DMF/2007/08" xmlns:sfc="http://schemas.microsoft.com/sqlserver/sfc/serialization/2007/08" xmlns:sml="http://schemas.serviceml.org/sml/2007/02" xmlns:xs="http://www.w3.org/2001/XMLSchema"><DMF:Parent><sfc:Reference sml:ref="true"><sml:Uri>/PolicyStore</sml:Uri></sfc:Reference></DMF:Parent><DMF:Expression type="string">&lt;Operator&gt;&lt;?char 13?&gt;
  &lt;TypeClass&gt;Bool&lt;/TypeClass&gt;&lt;?char 13?&gt;
  &lt;OpType&gt;NE&lt;/OpType&gt;&lt;?char 13?&gt;
  &lt;Count&gt;2&lt;/Count&gt;&lt;?char 13?&gt;
  &lt;Function&gt;&lt;?char 13?&gt;
    &lt;TypeClass&gt;Numeric&lt;/TypeClass&gt;&lt;?char 13?&gt;
    &lt;FunctionType&gt;ExecuteSql&lt;/FunctionType&gt;&lt;?char 13?&gt;
    &lt;ReturnType&gt;Numeric&lt;/ReturnType&gt;&lt;?char 13?&gt;
    &lt;Count&gt;2&lt;/Count&gt;&lt;?char 13?&gt;
    &lt;Constant&gt;&lt;?char 13?&gt;
      &lt;TypeClass&gt;String&lt;/TypeClass&gt;&lt;?char 13?&gt;
      &lt;ObjType&gt;System.String&lt;/ObjType&gt;&lt;?char 13?&gt;
      &lt;Value&gt;Numeric&lt;/Value&gt;&lt;?char 13?&gt;
    &lt;/Constant&gt;&lt;?char 13?&gt;
    &lt;Constant&gt;&lt;?char 13?&gt;
      &lt;TypeClass&gt;String&lt;/TypeClass&gt;&lt;?char 13?&gt;
      &lt;ObjType&gt;System.String&lt;/ObjType&gt;&lt;?char 13?&gt;
      &lt;Value&gt;&amp;lt;?char 13?&amp;gt;&lt;?char 13?&gt;
SET NOCOUNT ON&amp;lt;?char 13?&amp;gt;&lt;?char 13?&gt;
&amp;lt;?char 13?&amp;gt;&lt;?char 13?&gt;
SELECT COUNT(name)&amp;lt;?char 13?&amp;gt;&lt;?char 13?&gt;
FROM master.dbo.sysdatabases&amp;lt;?char 13?&amp;gt;&lt;?char 13?&gt;
WHERE name = ''distribution''&amp;lt;?char 13?&amp;gt;&lt;?char 13?&gt;
&amp;lt;?char 13?&amp;gt;&lt;?char 13?&gt;
SET NOCOUNT OFF&amp;lt;?char 13?&amp;gt;&lt;?char 13?&gt;
&lt;/Value&gt;&lt;?char 13?&gt;
    &lt;/Constant&gt;&lt;?char 13?&gt;
  &lt;/Function&gt;&lt;?char 13?&gt;
  &lt;Constant&gt;&lt;?char 13?&gt;
    &lt;TypeClass&gt;Numeric&lt;/TypeClass&gt;&lt;?char 13?&gt;
    &lt;ObjType&gt;System.Double&lt;/ObjType&gt;&lt;?char 13?&gt;
    &lt;Value&gt;0&lt;/Value&gt;&lt;?char 13?&gt;
  &lt;/Constant&gt;&lt;?char 13?&gt;
&lt;/Operator&gt;</DMF:Expression><DMF:Name type="string">EPM - Is Replicated</DMF:Name><DMF:Description type="string" /><DMF:Facet type="string">Server</DMF:Facet></DMF:Condition></data></document><document><docinfo><aliases><alias>/PolicyStore/PolicyCategory/EPM_b Performance</alias></aliases><sfc:version DomainVersion="3" /></docinfo><data><DMF:PolicyCategory xmlns:DMF="http://schemas.microsoft.com/sqlserver/DMF/2007/08" xmlns:sfc="http://schemas.microsoft.com/sqlserver/sfc/serialization/2007/08" xmlns:sml="http://schemas.serviceml.org/sml/2007/02" xmlns:xs="http://www.w3.org/2001/XMLSchema"><DMF:Parent><sfc:Reference sml:ref="true"><sml:Uri>/PolicyStore</sml:Uri></sfc:Reference></DMF:Parent><DMF:Name type="string">EPM: Performance</DMF:Name></DMF:PolicyCategory></data></document><document><docinfo><aliases><alias>/PolicyStore/ObjectSet/EPM - Replication Latency Greater Than 15 minutes__ObjectSet/TargetSet/Server_/Database</alias></aliases><sfc:version DomainVersion="3" /></docinfo><data><DMF:TargetSet xmlns:DMF="http://schemas.microsoft.com/sqlserver/DMF/2007/08" xmlns:sfc="http://schemas.microsoft.com/sqlserver/sfc/serialization/2007/08" xmlns:sml="http://schemas.serviceml.org/sml/2007/02" xmlns:xs="http://www.w3.org/2001/XMLSchema"><DMF:Levels><sfc:Collection><sfc:Reference sml:ref="true"><sml:Uri>/PolicyStore/ObjectSet/EPM - Replication Latency Greater Than 15 minutes__ObjectSet/TargetSet/Server_/Database/TargetSetLevel/Server_/Database</sml:Uri></sfc:Reference></sfc:Collection></DMF:Levels><DMF:Parent><sfc:Reference sml:ref="true"><sml:Uri>/PolicyStore/ObjectSet/EPM - Replication Latency Greater Than 15 minutes__ObjectSet</sml:Uri></sfc:Reference></DMF:Parent><DMF:TargetTypeSkeleton type="string">Server/Database</DMF:TargetTypeSkeleton><DMF:Enabled type="boolean">true</DMF:Enabled></DMF:TargetSet></data></document><document><docinfo><aliases><alias>/PolicyStore/Condition/Is master</alias></aliases><sfc:version DomainVersion="3" /></docinfo><data><DMF:Condition xmlns:DMF="http://schemas.microsoft.com/sqlserver/DMF/2007/08" xmlns:sfc="http://schemas.microsoft.com/sqlserver/sfc/serialization/2007/08" xmlns:sml="http://schemas.serviceml.org/sml/2007/02" xmlns:xs="http://www.w3.org/2001/XMLSchema"><DMF:Parent><sfc:Reference sml:ref="true"><sml:Uri>/PolicyStore</sml:Uri></sfc:Reference></DMF:Parent><DMF:Expression type="string">&lt;Operator&gt;&lt;?char 13?&gt;
  &lt;TypeClass&gt;Bool&lt;/TypeClass&gt;&lt;?char 13?&gt;
  &lt;OpType&gt;EQ&lt;/OpType&gt;&lt;?char 13?&gt;
  &lt;Count&gt;2&lt;/Count&gt;&lt;?char 13?&gt;
  &lt;Attribute&gt;&lt;?char 13?&gt;
    &lt;TypeClass&gt;Unsupported&lt;/TypeClass&gt;&lt;?char 13?&gt;
    &lt;Name&gt;Name&lt;/Name&gt;&lt;?char 13?&gt;
  &lt;/Attribute&gt;&lt;?char 13?&gt;
  &lt;Constant&gt;&lt;?char 13?&gt;
    &lt;TypeClass&gt;String&lt;/TypeClass&gt;&lt;?char 13?&gt;
    &lt;ObjType&gt;System.String&lt;/ObjType&gt;&lt;?char 13?&gt;
    &lt;Value&gt;master&lt;/Value&gt;&lt;?char 13?&gt;
  &lt;/Constant&gt;&lt;?char 13?&gt;
&lt;/Operator&gt;</DMF:Expression><DMF:Name type="string">Is master</DMF:Name><DMF:Description type="string">Confirms that the database name is master.</DMF:Description><DMF:Facet type="string">Database</DMF:Facet></DMF:Condition></data></document><document><docinfo><aliases><alias>/PolicyStore/ObjectSet/EPM - Replication Latency Greater Than 15 minutes__ObjectSet/TargetSet/Server_/Database/TargetSetLevel/Server_/Database</alias></aliases><sfc:version DomainVersion="3" /></docinfo><data><DMF:TargetSetLevel xmlns:DMF="http://schemas.microsoft.com/sqlserver/DMF/2007/08" xmlns:sfc="http://schemas.microsoft.com/sqlserver/sfc/serialization/2007/08" xmlns:sml="http://schemas.serviceml.org/sml/2007/02" xmlns:xs="http://www.w3.org/2001/XMLSchema"><DMF:Parent><sfc:Reference sml:ref="true"><sml:Uri>/PolicyStore/ObjectSet/EPM - Replication Latency Greater Than 15 minutes__ObjectSet/TargetSet/Server_/Database</sml:Uri></sfc:Reference></DMF:Parent><DMF:TargetSetLevelCondition><sfc:Reference sml:ref="true"><sml:Uri>/PolicyStore/Condition/Is master</sml:Uri></sfc:Reference></DMF:TargetSetLevelCondition><DMF:TargetTypeSkeleton type="string">Server/Database</DMF:TargetTypeSkeleton><DMF:LevelName type="string">Database</DMF:LevelName><DMF:Condition type="string">Is master</DMF:Condition></DMF:TargetSetLevel></data></document></instances></DMF:bufferData></xs:schema></data></document></definitions></xs:bufferSchema></model>