  <DataSets>
    <DataSet Name="GaugeData">
      <Query>
        <DataSourceName>PolicyDataDS</DataSourceName>
        <CommandText>WITH LastRunByPolicyCategory
AS
(SELECT MAX(PolicyEvalDate) AS 'LastRunPolicyEvalDate'
      ,ConfigurationGroup
      ,PolicyCategoryFilter
FROM   PolicyEval
GROUP BY ConfigurationGroup
        ,PolicyCategoryFilter
)
SELECT SUM(CAST(p.Result AS int)) AS 'Pass'
      ,SUM(CASE p.Result
          WHEN 0 THEN 1 END) AS 'Fail'
      ,COUNT(*) AS 'Total'
FROM  PolicyEval p
JOIN LastRunByPolicyCategory l
ON p.ConfigurationGroup = l.ConfigurationGroup
AND p.PolicyCategoryFilter = l.PolicyCategoryFilter
AND p.PolicyEvalDate = l.LastRunPolicyEvalDate
</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Pass">
          <DataField>Pass</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Fail">
          <DataField>Fail</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Total">
          <DataField>Total</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="GetEvalData">
      <Query>
        <DataSourceName>PolicyDataDS</DataSourceName>
        <CommandText>WITH LastRunByPolicyCategory
AS
(SELECT MAX(PolicyEvalDate) AS 'LastRunPolicyEvalDate'
      ,ConfigurationGroup
      ,PolicyCategoryFilter
FROM   PolicyEval
GROUP BY ConfigurationGroup
        ,PolicyCategoryFilter
)
SELECT p.PolicyEvalDate AS 'EvalID'
         ,CONVERT(char(10),p.PolicyEvalDate,101) As 'EvalDateDetail'
         ,p.PolicyCategoryFilter AS 'PolicyCategoryFilterDetail'
         ,p.ConfigurationGroup AS 'ConfigurationGroupDetail'
         ,p.PolicyName AS 'EvalPolicyDetail'
         ,COUNT(*) AS 'TotalDetail'
         ,SUM(CAST(p.Result AS int)) AS 'PassDetail'
         ,SUM(CASE p.Result WHEN 0 THEN 1 END) AS 'FailDetail'
   FROM   PolicyEval p
   JOIN LastRunByPolicyCategory l
   ON p.ConfigurationGroup = l.ConfigurationGroup
   AND p.PolicyCategoryFilter = l.PolicyCategoryFilter
   AND p.PolicyEvalDate = l.LastRunPolicyEvalDate
   GROUP BY p.PolicyCategoryFilter
           ,p.ConfigurationGroup
           ,p.PolicyEvalDate
           ,p.PolicyName
   ORDER BY p.ConfigurationGroup DESC
           ,p.PolicyCategoryFilter
           ,p.PolicyName</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="EvalID">
          <DataField>EvalID</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="EvalDateDetail">
          <DataField>EvalDateDetail</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="PolicyCategoryFilterDetail">
          <DataField>PolicyCategoryFilterDetail</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ConfigurationGroupDetail">
